{
  "expo": {
    "name": "Deliveroo Clone",
    "slug": "deliveroo-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/Logo/deliveroo-clone-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#00CCBB"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "ADD_YOUR_OWN_API_KEY_FOR_GOOGLE_MAPS"
        }
      },
      "package": "com.deliverooclone.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1b1942bc-acdd-4e25-93e9-6d11de8ecdfd"
      }
    }
  }
}
