{
  "expo": {
    "name": "Flavroo",
    "slug": "flavroo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image.png",
    "userInterfaceStyle": "light",
    "scheme": "flavroo",
    "splash": {
      "image": "./assets/images/Logo/deliveroo-clone-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#00CCBB"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flavroo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "pan",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCXM2CVmVkOsoIj3G7ZHnvZX-GycYcXMeU"
        }
      },
      "package": "com.flavroo.app",
    },
    "web": {
      "favicon": "./assets/favicon.png",

      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "1b1942bc-acdd-4e25-93e9-6d11de8ecdfd"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "useLegacyPackaging": true
          },
          "ios": {}
        }
      ]
    ]
  }
}
